    "lint-staged": "^10.5.0",
    "prettier": "^2.1.2",
    "serverless": "^2.9.0",
    "serverless-jetpack": "^0.10.7",
    "serverless-offline": "^6.8.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "apollo-server-lambda": "^2.19.0",
    "aws-sdk": "^2.783.0",
    "graphql": "^15.4.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --quiet --relative"
    }
